    - localhost.ptlogin2.qq.com
    - '+.srv.nintendo.net'
    - '+.stun.playstation.net'
    - '+.msftconnecttest.com'
    - '+.msftncsi.com'
    - '+.xboxlive.com'
    - 'msftconnecttest.com'
    - 'xbox.*.microsoft.com'
    - '*.battlenet.com.cn'
    - '*.battlenet.com'
    - '*.blzstatic.cn'
    - '*.battle.net'




#远程订阅
proxy-providers:
  aiport01_Subscription: &aiport01 #建立锚点
    type: http
    url: "你的第一个机场Clash订阅"
    interval: 86400
    path: ./providers/proxy/aiport01.yaml
    health-check:
      enable: true
      interval: 600
      url: https://www.gstatic.com/generate_204

#clah  p核正则
#   (A).*(B)         节点名既有 A 也有 B 
#   (A)|(B)          节点名有 A 或者 B   
#   ^(?!.*A)         节点名不含有 A 
#   ^(?!.*A).*(B)    节点名不含有 A 但含有 B
#   ^(?!.*(A|B))     节点名既不含有 A 也不含有 B

#订阅正则提取分组
#aiport01部分
  aiport01_Filter_All:
    <<: *aiport01

  aiport01_Filter_HK:
    <<: *aiport01
    filter: (?i)香港|🇭🇰|HK|Hong

  aiport01_Filter_TW:
    <<: *aiport01
    filter: (?i)台(湾|灣)|🇹🇼|TW|Taiwan

  aiport01_Filter_SG:
    <<: *aiport01
    filter: (?i)狮|獅|🇸🇬|加坡|SG|Singapore

  aiport01_Filter_US:
    <<: *aiport01
    filter: (?i)美(国|國)|🇺🇸|US|States|American

  aiport01_Filter_JP:
    <<: *aiport01
    filter: (?i)日本|🇯🇵|(东|東)京|JP|Japan



